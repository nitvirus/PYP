<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.nit.palampuryellowpages.Doctors">

    <RelativeLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/relativeLayout"
    android:layout_alignParentTop="true"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="41dp"></RelativeLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Dr Karan Singh"
        android:id="@+id/doc1_name"
        android:layout_below="@+id/relativeLayout"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="28dp"
        android:layout_toRightOf="@+id/relativeLayout"
        android:layout_toEndOf="@+id/relativeLayout" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="+(91)-9811051806"
        android:id="@+id/doc1_phone"
        android:layout_alignTop="@+id/imageButton"
        android:layout_alignRight="@+id/doc1_name"
        android:layout_alignEnd="@+id/doc1_name" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton"
        android:src="@android:drawable/ic_menu_call"
        android:layout_above="@+id/doc2_name"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Fortis Hospital"
        android:id="@+id/doc5_name"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/doc1_name"
        android:layout_alignEnd="@+id/doc1_name"
        android:layout_marginBottom="59dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton3"
        android:src="@android:drawable/ic_menu_call"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/doc5_name"
        android:layout_alignStart="@+id/doc5_name" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="+(91)-1892-242555, +(91)-8894724069"
        android:id="@+id/doc5_phone"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_alignLeft="@+id/doc3_phone"
        android:layout_alignStart="@+id/doc3_phone" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Dr. Vijay Choudhary"
        android:id="@+id/doc2_name"
        android:layout_above="@+id/imageButton4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton4"
        android:src="@android:drawable/ic_menu_call"
        android:layout_above="@+id/doc_name"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="(1894) 232745"
        android:id="@+id/doc2_phone"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_alignRight="@+id/doc1_phone"
        android:layout_alignEnd="@+id/doc1_phone"
        android:layout_alignParentEnd="false"
        android:layout_alignParentStart="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Kapila Nursing Home"
        android:id="@+id/doc_name"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/doc5_name"
        android:layout_alignStart="@+id/doc5_name"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton2"
        android:src="@android:drawable/ic_menu_call"
        android:layout_below="@+id/doc_name"
        android:layout_alignLeft="@+id/doc_name"
        android:layout_alignStart="@+id/doc_name"
        android:layout_alignParentEnd="false"
        android:layout_alignParentStart="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="(1894) 230734, (1894) 231022"
        android:id="@+id/doc3_phone"
        android:layout_below="@+id/doc_name"
        android:layout_alignRight="@+id/doc4_name"
        android:layout_alignEnd="@+id/doc4_name" />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Kaya Kalpa"
        android:id="@+id/doc4_name"
        android:layout_above="@+id/imageButton2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton2"
        android:src="@android:drawable/ic_menu_call"
        android:layout_above="@+id/doc5_name"
        android:layout_alignLeft="@+id/doc5_name"
        android:layout_alignStart="@+id/doc5_name" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="0189 423 5676"
        android:id="@+id/doc4_phone"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_alignRight="@+id/doc4_name"
        android:layout_alignEnd="@+id/doc4_name" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="List of doctors around Palampur:"
        android:id="@+id/intro_doc"
        android:layout_above="@+id/doc1_name"
        android:layout_toRightOf="@+id/relativeLayout"
        android:layout_toEndOf="@+id/relativeLayout" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton"
        android:layout_toEndOf="@+id/imageButton"></LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"></LinearLayout>

</RelativeLayout>
